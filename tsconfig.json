{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": "./src",
    "paths": {
      "@icons/*": ["icons/*"],
      "@assets/*": ["assets/*"],
      "@components/*": ["components/*"],
      "@layouts/*": ["layouts/*"],
      "@data/*": ["data/*"],
      "@pages/*": ["pages/*"]
    },
    "jsx": "preserve",
    "jsxImportSource": "solid-js"
  }
  // "exclude": ["node_modules", "dist", "public"]
}
